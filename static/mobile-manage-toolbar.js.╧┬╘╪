(function($,g){var from_data=null,from=null,page=SOHUZ.page.page_id,before_url=document.referrer,search=window.location.search,before_from=null,search_from=null;if(before_url.indexOf("&from=")>0){var match=/^(site-list|tpl|preview).*/gi.exec(before_url.substring(before_url.indexOf("&from=")+6));before_from=match?match[0]=="site-list"?"default":match[0]:null}if(search){if(search.indexOf("from=")>=0){search_from=search.substring(search.indexOf("from=")+5);if($.inArray(search_from,["tpl","preview"])<0&&search_from.length>=0){search=""}}if($.inArray(search_from,["tpl","preview"])<0){search_from=null}if(search.indexOf("from=")>=0){try{localStorage.setItem("preview_from",JSON.stringify({from:search_from?search_from:"default",page:page}))}catch(e){console.log("Browser not support write localStorage")}}}from=before_from?before_from:search_from?search_from:"default";if("localStorage"in window&&window["localStorage"]!==null){from_data=JSON.parse(localStorage.getItem("preview_from"));if(from_data&&from_data.length!=0){from=from_data.from;page=from_data.page}}var pic="//pic.kuaizhan.com/g1/M01/7D/5F/CgpQU1bzlEyAXthpAAAJBBEBpTA423.png";var manage_url=SOHUZ.CONF.url_v2+"/sites/"+SOHUZ.page.site_id+"/dashboard",from_tpl={default:{left:'<span class="avatar" data-role="avatar"><img src="'+pic+'"/></span>',msg:"Hi，站长 ：）",btn_msg:"进入管理",url:manage_url},tpl:{left:'<span class="k-i-warning"></span>',msg:"此为模版页面,请勿分享",btn_msg:"继续选择",url:SOHUZ.CONF.url_v2+"/template/list"},preview:{left:'<span class="k-i-warning"></span>',msg:"此为预览页面,请勿分享",btn_msg:"继续编辑",url:SOHUZ.CONF.url_v2+"/pages/"+page+"/edit"}};var footer_manage='<div class="page-toolbar">'+'<div class="inner">'+'<div class="left">'+'<a href="javascript:;" class="warning">'+'<div class="icon">'+from_tpl[from].left+"</div>"+'<span class="msg">'+from_tpl[from].msg+"</span>"+"</a>"+"</div>"+'<div class="right">'+'<a href="'+from_tpl[from].url+'" class="ui-button confirm">'+from_tpl[from].btn_msg+"</a>"+"</div>"+"</div>"+"</div>";$.ajax